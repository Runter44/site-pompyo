{{ form_start(form, {'attr': {'onsubmit': 'return confirm("Envoyer le message ?");'}}) }}
<div class="form-row">
    <div class="form-group col">
        {{ form_row(form.prenom) }}
    </div>
    <div class="form-group col">
        {{ form_row(form.nom) }}
    </div>
</div>
<div class="form-row">
    <div class="form-group col">
        {{ form_row(form.email) }}
    </div>
</div>
<div class="form-row">
    <div class="form-group col">
        {{ form_row(form.objet) }}
    </div>
</div>
<div class="form-row">
    <div class="form-group col">
        {{ form_row(form.message) }}
    </div>
</div>
{{ form_end(form) }}